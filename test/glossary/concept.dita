<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE concept PUBLIC "-//OASIS//DTD DITA Concept//EN" "concept.dtd">
<concept id="concept">
    <title>Concept</title>
    <shortdesc>
       Concept with a term.
    </shortdesc>
    <conbody>
        <ul>
            <li>
               <codeph>term</codeph> (empty) with <codeph>keyref</codeph> attribute: <term keyref="usbfd"/>
            </li>
            <li>
               <codeph>term</codeph> (filled) with <codeph>keyref</codeph> attribute: <term keyref="usbfd">term</term>
            </li>
            <li>
               <codeph>abbreviated-form</codeph>:
                <abbreviated-form keyref="usbfd"/>
            </li>
        </ul>
    </conbody>
</concept>